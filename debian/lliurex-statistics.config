#!/bin/sh
# config script for lliurex-statistics
#
# see: dh_installdeb(1)

set -e

. /usr/share/debconf/confmodule
db_version 2.0
#db_capb backup

RET=""    
db_get lliurex-statistics/unattended || true
if [ "$RET" = "false" ] || [ -n "$DEBCONF_RECONFIGURE" ]; then
        db_input high lliurex-statistics/acknowledge || true
        db_go
fi

RET=""
db_get lliurex-statistics/acknowledge

#DEBHELPER#

exit 0
